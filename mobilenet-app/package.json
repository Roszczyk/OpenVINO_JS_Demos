{
    "name": "mobilenet-app",
    "version": "1.0.0",
    "description": "Run MobileNet Classification with OpenVINO",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "build": "electron-builder"
    },
    "author": "Mikolaj Roszczyk (Intel)",
    "license": "MIT",
    "dependencies": {
      "openvino-node": "^2024.2.0",
      "@napi-rs/canvas": "^0.1.52",
      "rimraf": "^6.0.1",
      "eslint": "^8.39.0",
      "https-proxy-agent": "^7.0.2",
      "opencv-wasm": "^4.3.0-10",
      "perf_hooks": "^0.0.1"
    },
    "devDependencies": {
      "electron": "^31.3.1",
      "electron-builder": "^24.13.3",
      "electron-rebuild": "^3.2.9"
    },
    "build": {
      "appId": "com.yourname.mobilenetopenvinoapp",
      "productName": "MobileNetOpenVINOApp",
      "files": [
        "main.js",
        "preload.js",
        "jobs.js",
        "renderer.js",
        "helpers.js",
        "index.html",
        "style.css",
        "models/mobilenet-ssd.xml",
        "models/mobilenet-ssd.bin",
        "node_modules/**/*"
      ],
      "asarUnpack": [
        "models/**/*"
      ],
      "directories": {
        "buildResources": "build"
      },
      "win": {
        "target": "nsis",
        "icon": "build/icon.ico"
      }
    }
  }